WINSCW ARMV5 GCCE


#ifdef SBSV2

PRJ_EXTENSIONS
	
	START EXTENSION s60/mifconv
	OPTION TARGETFILE profileswitcher_0xE6DC77AC.mif
	OPTION HEADERFILE profileswitcher_0xE6DC77AC.mbg
	OPTION SOURCEDIR ../gfx
	OPTION SOURCES -c32 qgn_menu_ProfileSwitcher
	END
	
	// help compiler is not always available; see similar comment in *.pkg file
